/* pages/abctjfx/abctjfx.wxss */
.main_wrap {
    width: 100%;
    margin: 0 auto;
    box-sizing: border-box;
    padding: 20rpx 20rpx 80rpx;
}
.head {
    background-color: white;
    border-radius: 8rpx;
    padding: 32rpx 32rpx 54rpx;
}
.title {
    width: 100%;
    display: flex;
    height: 48rpx;
    align-items: center;
    margin-bottom: 40rpx;
    font-size: 36rpx;
}
.title view {
    width: 6rpx;
    height: 36rpx;
    background-color: #00ce9f;
    margin-right: 20rpx;
}
.nav_box {
    width: 100%;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
}
.box {
    position: relative;
    width: 200rpx;
}
.box_block {
    width: 160rpx;
    margin: 0 auto;
    height: 160rpx;
    transform: rotate(45deg);
    border-radius: 50%;
}
.box1 {
    border: 10rpx solid #ff9582;
    border-top-color: #ff0e0e;
}
.box2 {
    border: 10rpx solid #fff77c;
    border-top-color: #ffe400;
}
.box3 {
    border: 10rpx solid #40ffd3;
    border-top-color: #00ce9f;
}
.box text {
    display: block;
    position: absolute;
    width: 100%;
    height: 180rpx;
    line-height: 180rpx;
    font-size: 40rpx;
    text-align: center;
    top: 0;
    left: 0;
}
.tex1 {
    color: #ff0e0e;
}
.tex2 {
    color: #ffe400;
}
.tex3 {
    color: #00ce9f;
}
.box_tit {
    font-size: 28rpx;
    text-align: center;
    margin-top: 20rpx;
}
.tit1 {
    color: #ff0e0e;
}
.tit2 {
    color: #ffe400;
}
.tit3 {
    color: #00ce9f;
}

.banner {
    padding: 24rpx 0;
  }
  .ban_input {
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
    height: 92rpx;
    background-color: white;
    border-radius: 46rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
  
  }
  .search {
    width: calc(100% - 100rpx)
  }
  .search input {
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
    outline: none;
    background: none;
  }
  .ser_img {
    width: 50rpx;
  }
  .ser_img image {
    width: 100%;
    display: block;
  }
  .main {
      padding: 0 20rpx;
    }
    .main_name {
        width: 100%;
        margin-bottom: 20rpx;
    }
    .titles {
        height: 88rpx;
        width: 100%;
        display: flex;
        justify-content: space-between;
        align-items: center;
        background-color: white;
        border-radius: 8rpx;
    }
    .text {
        height: 100%;
        padding-left: 32rpx;
        line-height: 88rpx;
        font-size: 34rpx;
    }
    .box_img2 {
        height: 40rpx;
        padding-right: 32rpx;
    }
    .box_img2 image {
        height: 100%;
        display: block;
    }